{
  "extends": "../../tsconfig.app.json",
  "files": [],
  "references": [
    {
      "path": "../../tsconfig.node.json"
    }
  ],

  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/**/*.ts", "src/**/*.d.ts"],
  "exclude": ["src/**/__tests__/*", "node_modules"],
  "compilerOptions": {
    "allowJs": true,
    "composite": true,
    "noEmit": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    }
  },
}
